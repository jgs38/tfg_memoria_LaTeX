@article{article:Nuevo_2012,
    author={J. González-Nuevo and A. Lapi and S. Fleuren and S. Bressan and L. Danese and G. De Zotti and M. Negrello and Z.-Y. Cai and L. Fan and W. Sutherland and M. Baes and A. J. Baker and D. L. Clements and A. Cooray and H. Dannerbauer and L. Dunne and S. Dye and S. Eales and D. T. Frayer and A. I. Harris and R. Ivison and M. J. Jarvis and M. J. Michałowski and M. López-Caniego and G. Rodighiero and K. Rowlands and S. Serjeant and D. Scott and P. van der Werf and R. Auld and S. Buttiglione and A. Cava and A. Dariush and J. Fritz and R. Hopwood and E. Ibar and S. Maddox and E. Pascale and M. Pohlen and E. Rigby and D. Smith and P. Temi},
    title={Herschel–ATLAS: Toward a Sample of ~1000 Strongly Lensed Galaxies},
    journal={The Astrophysical Journal},
    volume={749},
    number={1},
    pages={65},
    url={http://stacks.iop.org/0004-637X/749/i=1/a=65},
    year={2012},
    abstract={While the selection of strongly lensed galaxies (SLGs) with 500 μm flux density S 500 > 100 mJy has proven to be rather straightforward, for many applications it is important to analyze samples larger than the ones obtained when confining ourselves to such a bright limit. Moreover, only by probing to fainter flux densities is it possible to exploit strong lensing to investigate the bulk of the high- z star-forming galaxy population. We describe HALOS (the Herschel -ATLAS Lensed Objects Selection), a method for efficiently selecting fainter candidate SLGs, reaching a surface density of ##IMG## [http://ej.iop.org/icons/Entities/sime.gif] {sime} 1.5-2 deg –2 , i.e., a factor of about 4-6 higher than that at the 100 mJy flux limit. HALOS will allow the selection of up to ~1000 candidate SLGs (with amplifications μ ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 2) over the full H-ATLAS survey area. Applying HALOS to the H-ATLAS Science Demonstration Phase field ( ##IMG## [http://ej.iop.org/icons/Entities/sime.gif] {sime} 14.4 deg 2 ) we find 31 candidate SLGs, whose candidate lenses are identified in the VIKING near-infrared catalog. Using the available information on candidate sources and candidate lenses we tentatively estimate a ##IMG## [http://ej.iop.org/icons/Entities/sime.gif] {sime} 72\% purity of the sample. As expected, the purity decreases with decreasing flux density of the sources and with increasing angular separation between candidate sources and lenses. The redshift distribution of the candidate lensed sources is close to that reported for most previous surveys for lensed galaxies, while that of candidate lenses extends to redshifts substantially higher than found in the other surveys. The counts of candidate SLGs are also in good agreement with model predictions. Even though a key ingredient of the method is the deep near-infrared VIKING photometry, we show that H-ATLAS data alone allow the selection of a similarly deep sample of candidate SLGs with an efficiency close to 50\%; a slightly lower surface density ( ##IMG## [http://ej.iop.org/icons/Entities/sime.gif] {sime} 1.45 deg –2 ) can be reached with a ~70\% efficiency.}
}

@article{article:Lapi_2011,
    author={A. Lapi and J. González-Nuevo and L. Fan and A. Bressan and G. De Zotti and L. Danese and M. Negrello and L. Dunne and S. Eales and S. Maddox and R. Auld and M. Baes and D. G. Bonfield and S. Buttiglione and A. Cava and D. L. Clements and A. Cooray and A. Dariush and S. Dye and J. Fritz and D. Herranz and R. Hopwood and E. Ibar and R. Ivison and M. J. Jarvis and S. Kaviraj and M. López-Caniego and M.Massardi and M. J. Michałowski and E. Pascale and M. Pohlen and E. Rigby and G. Rodighiero and S. Serjeant and D. J. B. Smith and P. Temi and J. Wardlow and P. van der Werf},
    title={Herschel-ATLAS Galaxy Counts and High-redshift Luminosity Functions: The Formation of Massive Early-type Galaxies},
    journal={The Astrophysical Journal},
    volume={742},
    number={1},
    pages={24},
    url={http://stacks.iop.org/0004-637X/742/i=1/a=24},
    year={2011},
    abstract={Exploiting the Herschel Astrophysical Terahertz Large Area Survey Science Demonstration Phase survey data, we have determined the luminosity functions (LFs) at rest-frame wavelengths of 100 and 250 μm and at several redshifts z ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 1, for bright submillimeter galaxies with star formation rates (SFRs) ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 100 M ☉ yr –1 . We find that the evolution of the comoving LF is strong up to z ##IMG## [http://ej.iop.org/icons/Entities/ap.gif] {≈} 2.5, and slows down at higher redshifts. From the LFs and the information on halo masses inferred from clustering analysis, we derived an average relation between SFR and halo mass (and its scatter). We also infer that the timescale of the main episode of dust-enshrouded star formation in massive halos ( M H ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 3 × 10 12 M ☉ ) amounts to ~7 × 10 8 yr. Given the SFRs, which are in the range of 10 2 -10 3 M ☉ yr –1 , this timescale implies final stellar masses of the order of 10 11 -10 12 M ☉ . The corresponding stellar mass function matches the observed mass function of passively evolving galaxies at z ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 1. The comparison of the statistics for submillimeter and UV-selected galaxies suggests that the dust-free, UV bright phase is ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 10 2 times shorter than the submillimeter bright phase, implying that the dust must form soon after the onset of star formation. Using a single reference spectral energy distribution (SED; the one of the z ##IMG## [http://ej.iop.org/icons/Entities/ap.gif] {≈} 2.3 galaxy SMM J2135-0102), our simple physical model is able to reproduce not only the LFs at different redshifts >1 but also the counts at wavelengths ranging from 250 μm to ##IMG## [http://ej.iop.org/icons/Entities/ap.gif] {≈} 1 mm. Owing to the steepness of the counts and their relatively broad frequency range, this result suggests that the dispersion of submillimeter SEDs of z > 1 galaxies around the reference one is rather small.}
}

