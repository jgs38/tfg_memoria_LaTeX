@article{article:annz,
  author={Adrian A. Collister and Ofer Lahav},
  title={ANN z : Estimating Photometric Redshifts Using Artificial Neural Networks},
  journal={Publications of the Astronomical Society of the Pacific},
  volume={116},
  number={818},
  pages={345},
  url={http://stacks.iop.org/1538-3873/116/i=818/a=345},
  year={2004},
  abstract={We introduce ANN z , a freely available software package for photometric redshift estimation using artificial neural networks. ANN z learns the relation between photometry and redshift from an appropriate training set of galaxies for which the redshift is already known. Where a large and representative training set is available, ANN z is a highly competitive tool when compared with traditional template‐fitting methods. The ANN z package is demonstrated on the Sloan Digital Sky Survey Data Release 1, and for this particular data set the rms redshift error in the range 0≲z≲0.7 is σ rms = 0.023. Nonideal conditions (spectroscopic sets that are small or brighter than the photometric set for which redshifts are required) are simulated, and the impact on the photometric redshift accuracy is assessed.2}
}

